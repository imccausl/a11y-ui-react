{
    "extends": [
        "@imccausl/eslint-config"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "project": "./tsconfig.json"
    },
    "ignorePatterns": [
        "dist",
        "node_modules",
        "pnp.*",
        ".yarn"
    ]
}