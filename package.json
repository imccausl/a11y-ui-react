    "eslint-config-turbo": "^0.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "latest",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-turbo": "^0.0.4",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "monodeploy": "^3.3.1",
    "prettier": "^2.7.1",
    "prettier-eslint-config": "^1.0.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
