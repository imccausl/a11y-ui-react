    "@types/react-dom": "17.0.2"
  },
  "storybook-deployer": {
    "gitUsername": "Documentation Deployer",
    "gitEmail": "imccausl@gmail.com",
    "commitMessage": "chore(Deploy): Deploy docs to GitHub Pages"
  }
