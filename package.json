{
  "name": "a11y-react",
  "version": "1.0.0",
  "description": "Accessible UI component examples for reference",
  "main": "lib/index.js",
  "author": "Ian McCausland <imccausl@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.4",
  "workspaces": {
    "packages": [
      "packages/*",
      "design-docs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imccausl/a11y-ui-react.git"
  },
  "scripts": {
    "g:tsc": "cd $INIT_CWD && tsc",
    "start:dev": "start-storybook -p 6006",
    "docs:deploy": "yarn docs:build && storybook-to-ghpages --existing-output-dir=.out --ci",
    "docs:build": "build-storybook -o .out -s .storybook/static",
    "g:test": "vitest",
    "g:coverage": "vitest --coverage",
    "commit-msg": "commitlint --edit",
    "prepack": "turbo run build --cache-dir=\".turbo\"",
    "test": "turbo run test --cache-dir=\".turbo\"",
    "lint": "turbo run lint --cache-dir=\".turbo\"",
    "typecheck": "turbo run typecheck --cache-dir=\".turbo\""
  },
