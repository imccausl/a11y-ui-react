    "@babel/preset-typescript": "^7.18.6",
    "@babel/types": "^7.19.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@mdx-js/react": "^2.1.3",
    "@monodeploy/plugin-github": "^0.6.2",
    "@storybook/addon-a11y": "^6.5.13",
